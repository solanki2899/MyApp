<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/walpaper"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingBottom="30dp">

            <TextView
                android:id="@+id/aboutusdesc"
                style="@android:style/Widget.Holo.Light.TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/unifrakturmaguntia"
                android:foregroundGravity="center"
                android:gravity="center"
                android:paddingBottom="20dp"
                android:text="Batch 2022"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Headline3"
                android:textColor="@color/colorAccent"
                android:textStyle="bold" />





            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                app:cardBackgroundColor="@color/design_default_color_primary_dark"
                app:cardCornerRadius="10dp"
                app:contentPaddingLeft="25dp"
                app:contentPaddingRight="25dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/design_default_color_primary_dark"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView11"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="30dp"
                        android:scaleType="center"
                        app:srcCompat="@drawable/sunil"
                        tools:srcCompat="@drawable/sunil" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/baumans"
                        android:gravity="center"
                        android:paddingTop="5dp"
                        android:text="SUNIL SOLANKI"
                        android:textColor="@color/normal"
                        android:textSize="20sp" />
                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                app:cardBackgroundColor="@color/design_default_color_primary_dark"
                app:cardCornerRadius="10dp"
                app:contentPaddingLeft="15dp"
                app:contentPaddingRight="15dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="30dp"
                        app:srcCompat="@drawable/mrinal"
                        tools:srcCompat="@drawable/mrinal" />

                    <TextView
                        android:id="@+id/textview1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/baumans"
                        android:gravity="center"
                        android:padding="5dp"
                        android:paddingTop="30dp"
                        android:text="MRINAL SATYAWANSHI"
                        android:textAlignment="center"
                        android:textColor="@color/normal"
                        android:textSize="20sp" />
                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginTop="20dp"
                app:cardBackgroundColor="@color/design_default_color_primary_dark"
                app:cardCornerRadius="10dp"
                app:contentPaddingLeft="25dp"
                app:contentPaddingRight="25dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="30dp"
                        app:srcCompat="@drawable/anshika"
                        tools:srcCompat="@drawable/anshika" />

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/baumans"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="ANSHIKA PANDEY"
                        android:textColor="@color/normal"
                        android:textSize="20sp" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                app:cardBackgroundColor="@color/design_default_color_primary_dark"
                app:cardCornerRadius="10dp"
                app:contentPaddingLeft="25dp"
                app:contentPaddingRight="25dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="30dp"
                        app:srcCompat="@drawable/rakshat"
                        tools:srcCompat="@drawable/rakshat" />

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/baumans"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="RAKSHAT PURANIK"
                        android:textColor="@color/normal"
                        android:textSize="20sp" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                app:cardBackgroundColor="@color/design_default_color_primary_dark"
                app:cardCornerRadius="10dp"
                app:contentPaddingLeft="25dp"
                app:contentPaddingRight="25dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="30dp"
                        app:srcCompat="@drawable/gaurav"
                        tools:srcCompat="@drawable/gaurav" />

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/baumans"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="GAURAV PRAJAPAT"
                        android:textColor="@color/normal"
                        android:textSize="20sp" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                app:cardBackgroundColor="@color/design_default_color_primary_dark"
                app:cardCornerRadius="10dp"
                app:contentPaddingLeft="25dp"
                app:contentPaddingRight="25dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="30dp"
                        app:srcCompat="@drawable/twinkla"
                        tools:srcCompat="@drawable/twinkla" />

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/baumans"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="TWINKLE AGARWAL"
                        android:textColor="@color/normal"
                        android:textSize="20sp" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                app:cardBackgroundColor="@color/design_default_color_primary_dark"
                app:cardCornerRadius="10dp"
                app:contentPaddingLeft="25dp"
                app:contentPaddingRight="25dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="30dp"
                        app:srcCompat="@drawable/sumit"
                        tools:srcCompat="@drawable/sumit" />

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/baumans"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="SUMIT NAGAR"
                        android:textColor="@color/normal"
                        android:textSize="20sp" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                app:cardBackgroundColor="@color/design_default_color_primary_dark"
                app:cardCornerRadius="10dp"
                app:contentPaddingLeft="25dp"
                app:contentPaddingRight="25dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView8"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="30dp"
                        app:srcCompat="@drawable/mayank"
                        tools:srcCompat="@drawable/mayank" />

                    <TextView
                        android:id="@+id/textView8"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/baumans"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="MAYANK SHARMA"
                        android:textColor="@color/normal"
                        android:textSize="20sp" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>




</RelativeLayout>